{
  "name": "@bartlss/mui-virtualized-grid",
  "version": "1.0.0",
  "description": "A Material-UI virtualization module utilizing Material-UI's Grid, List and ListItem components",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartlss/mui-virtualized-grid.git"
  },
  "keywords": [
    "Material-ui",
    "mui",
    "virtualize",
    "virtualized",
    "grid",
    "list",
    "virtualization",
    "material",
    "ui"
  ],
  "author": "Sean Bartlett",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bartlss/mui-virtualized-grid/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/bartlss/mui-virtualized-grid#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "ts-jest": "^26.5.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.2"
  }
}
